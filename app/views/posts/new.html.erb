<h1>画像投稿フォーム</h1>
<%= form_with model: @post, local:true do |f| %>
  <h4>画像</h4>
  <%= f.attachment_field :image %>
  <h4>ショップ名</h4>
  <%= f.text_field :shop_name %>
  <h4>ラーメン名</h4>
  <%= f.text_field :food_name %>
  <h4>ジャンル名</h4>
  <%= f.select :genre, Post.genres.keys, {prompt: '選択してください'}, class: 'form-control btn btn-info' %>
  <!--<= f.text_field :genre %>-->
  <h4>値段</h4>
  <%= f.text_field :price %>
  <h4>説明</h4>
  <%= f.text_area :introduction %>
  <h4>都道府県</h4>
  <%= f.select :prefecture, Post.prefectures.keys, {prompt: '選択してください'}, class: 'form-control btn btn-info' %>
  <!--<= f.select :prefecture, Post.prefectures.keys.map { |k| [I18n.t("enums.post.prefectures.#{k}"), k]},class: "prefecture-select " %>-->
  <%= f.submit '投稿' %>
<% end %>